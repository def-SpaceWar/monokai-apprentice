        "foreground": "#5AB8D9"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#5AB8D9"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
